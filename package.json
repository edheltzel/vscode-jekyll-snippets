{
  "name": "vscode-jekyll-snippets",
  "version": "0.9.0",
  "publisher": "ginfuru",
  "displayName": "Jekyll Snippets",
  "description": "Jekyll snippets for Visual Studio Code",
  "icon": "images/jekyll-logo.png",
  "galleryBanner": {
    "color": "#333333",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.5.x"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "snippets",
    "markdown",
    "liquid",
    "jekyll"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "homepage": "https://github.com/ginfuru/vscode-jekyll-snippets",
  "repository": {
    "type": "git",
    "url": "https://github.com/ginfuru/vscode-jekyll-snippets.git"
  },
  "bugs": "https://github.com/ginfuru/vscode-jekyll-snippets/issues",
  "contributes": {
    "snippets": [
      {
        "language": "jekyll",
        "path": "./snippets/jekyll.json"
      },
      {
        "language": "html",
        "path": "./snippets/jekyll.json"
      },
      {
        "language": "markdown",
        "path": "./snippets/jekyll.json"
      }
    ],
    "commands": [
      {
        "command": "jekyll.build",
        "title": "Jekyll: Build"
      },
      {
        "command": "jekyll.getVersion",
        "title": "Jekyll: Version"
      },
      {
        "command": "jekyll.downloadTheme",
        "title": "Jekyll: Download Themes"
      },
      {
        "command": "jekyll.newPost",
        "title": "Jekyll: New Post"
      },
      {
        "command": "jekyll.newSite",
        "title": "Jekyll: New Site"
      },
      {
        "command": "jekyll.setTheme",
        "title": "Jekyll: Set Theme"
      },
      {
        "command": "jekyll.startServer",
        "title": "Jekyll: Start Server"
      },
      {
        "command": "jekyll.stopServer",
        "title": "Jekyll: Stop Server"
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "opn": "^4.0.2"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.34",
    "@types/node": "^6.0.53",
    "mocha": "^2.3.3",
    "tslint": "^4.2.0",
    "typescript": "^2.1.0",
    "vscode": "^1.0.3"
  },
  "extensionDependencies": [
    "ginfuru.ginfuru-vscode-jekyll-syntax"
  ],
  "__metadata": {
    "id": "7891ba3a-fe11-4e55-bf8f-21479bed022c",
    "publisherId": "e4159f93-1b6e-4dd8-b879-3bc62c30dad0",
    "publisherDisplayName": "ginfuru"
  }
}
